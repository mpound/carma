    <Container name="SpectralLineCorrelator"  priority="vital">
       <MonitorPoint name="numBands" type="int" persistent="true">
         <shortName>NumBands</shortName>
         <description>
             Number of bands in this correlator
         </description>
       </MonitorPoint>
       <MonitorPoint name="name" type="string" persistent="true">
         <shortName>Name</shortName>
         <description>
             Correlator name (e.g. SpectralLine, Wideband)
         </description>
       </MonitorPoint>

       <MonitorPoint name="modeDesc" type="string" persistent="true">
         <shortName>mode</shortName>
         <description>
          String describing the correlator band set up. It will be a
          concatenation of the individual bandwidths for one sideband
          (the other sideband is necessarily identical), of the form
          BW1-BW2-...-BWn.  For instance if the correlator bands are
          BW1=500MHz, BW2=62MHz, BW3=8MHz, the string would be 500-62-8.
          For missing bands, X will be used, e.g. 500-62-X.  This monitor
          point will correspond to the MIRIAD header variable 'slcmode'.
         </description>
       </MonitorPoint>

       <MonitorPoint name="controllingSubarray" type="string" persistent="true">
         <shortName>Controlling Subarray</shortName>
         <description>
         String indicating which subarray is controlling the correlator.
         </description>
       </MonitorPoint>

