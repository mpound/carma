<!-- Source Information -->

       <MonitorPoint name="source" type="string" persistent="true"
                     priority="vital">
          <shortName>Source</shortName>
          <longName>Source Name</longName>
          <description>
            Current sourcename including pseudo sourcenames such as
            NONE and NOISE.
          </description>
        </MonitorPoint>

       <MonitorPoint name="phaseCenterRa" type="double" persistent="true"
           priority="vital">
         <shortName>Phase Center RA</shortName>
         <description> Right ascension of the phase center in
                       the mean equinox (i.e., J2000).
                       This corresponds to the MIRIAD header variable 'ra'.
         </description>
         <units>radians</units>
       </MonitorPoint>
       <MonitorPoint name="phaseCenterDec" type="double" persistent="true"
           priority="vital">
         <shortName>Phase Center Dec</shortName>
         <description>
                Declination of the phase center in the mean equinox
                (i.e., J2000). This corresponds to the MIRIAD header
                variable 'dec'.
        </description>
         <units>radians</units>
       </MonitorPoint>
       <MonitorPoint name="phaseCenterAppRa" type="double" persistent="true"
           priority="vital">
         <shortName>Phase Cntr App RA</shortName>
         <description>
                Phase center apparent right ascension as
                viewed from the array reference position. This value
                is in the current equinox, includes any equatorial offsets,
                and corresponds to the MIRIAD header variable 'obsra'.
         </description>
         <units>radians</units>
       </MonitorPoint>
       <MonitorPoint name="phaseCenterAppDec" type="double" persistent="true"
           priority="vital">
         <shortName>Phase Cntr App Dec</shortName>
         <description>
                Phase center apparent declination as viewed from
                the array reference position. This value is in the
                current equinox, includes any equatorial offsets,
                and corresponds to the MIRIAD header variable 'obsdec'.
         </description>
         <units>radians</units>
       </MonitorPoint>
       <MonitorPoint name="meanEquinox" type="double" persistent="true"
           priority="vital">
         <shortName>Mean Equinox</shortName>
         <description>
              Mean equinox for the pointing and phase center. This is the
              equinox used for the monitor points phaseCenterRa and
              phaseCenterDec.
         </description>
         <units>year</units>
       </MonitorPoint>
       <MonitorPoint name="planetMajorAxis" type="double" persistent="true"
           priority="vital">
         <shortName>Pl Maj</shortName>
         <description> Planet diameter major axis </description>
         <units>arcsec</units>
       </MonitorPoint>
       <MonitorPoint name="planetMinorAxis" type="double" persistent="true"
           priority="vital">
         <shortName>Pl Min</shortName>
         <description> Planet diameter minor axis </description>
         <units>arcsec</units>
       </MonitorPoint>
       <MonitorPoint name="planetTemperature" type="double" persistent="true"
           priority="vital">
         <shortName>Pl Temp</shortName>
         <description>
               Planet brightness temperature (blackbody) at the observed
               frequency.
         </description>
         <units>K</units>
       </MonitorPoint>
       <MonitorPoint name="planetAngle" type="double" persistent="true"
           priority="vital">
         <shortName>Pl Angle</shortName>
         <description>
               Planet major axis rotation angle with respect to RA/DEC.
               See Astronomical Almanac Section E3.
         </description>
         <units>deg</units>
       </MonitorPoint>

    <MonitorPoint name="dopplerSource" type="string" persistent="true"
           priority="vital">
      <shortName>Dop Src</shortName>
      <longName>Doppler tracking source</longName>
      <description>
            Source which is being use for Doppler tracking of the
            local oscillator. It may be
            different than the sky source being observed.
      </description>
    </MonitorPoint>

       <MonitorPoint name="velocity" type="double" persistent="true"
           priority="vital">
         <shortName>Vel</shortName>
         <description>
                Systemic velocity of the doppler source in the given reference
                frame, e.g. VLSR.
         </description>
         <units>km/s</units>
         <precision>2</precision>
       </MonitorPoint>

       <MonitorPoint name="velFrame" type="enum" persistent="true"
           priority="vital">
         <shortName>Vel Frame</shortName>
         <description>
                The reference frame for the velocity.
         </description>
         <enum name="LSR"/>
         <description>
                Local standard of rest
         </description>
         <enum name="HELIO"/>
         <description>
                The sun
         </description>
         <enum name="OBSERV"/>
         <description>
                The observatory (specifically, the array center).
                This is also known as Radial velocity.
         </description>
         <enum name="PLANET"/>
         <description>
                The planet currently being observed
         </description>
       </MonitorPoint>

       <MonitorPoint name="velDef" type="enum" persistent="true"
           priority="vital">
         <shortName>Vel Def</shortName>
         <description>
                The definition of velocity used to define the doppler shift.
         </description>
         <enum name="RADIO"/>
         <description>
                The definition used by radio astronomers, where a velocity
            change results in a frequency shift.
         </description>
         <enum name="OPTICAL"/>
         <description>
            The definition used by optical astronomers, where a velocity
            change results in a wavelength shift.
         </description>
         <enum name="RELATIVISTIC"/>
         <description>
                Use the relativistic equation for the doppler. Use with extreme
                caution as this is normally not used by astronomers.
         </description>
       </MonitorPoint>

       <MonitorPoint name="velObservatory" type="double" persistent="true"
           priority="vital">
         <shortName>Vel Radial</shortName>
         <description>
            The velocity of the observatory with respect to the Doppler
            source, also called the radial velocity.
            Used to compute LO frequency with the Doppler equation.
         </description>
         <units>km/s</units>
         <precision>2</precision>
       </MonitorPoint>

<!-- END Source Information -->
