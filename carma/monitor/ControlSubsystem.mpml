<!-- ============================================================= -->
<!-- Subarray commands -->

      <Container name="Commands" priority="vital">
        <description>
            Only the parameters needed to restore the state of the subarray
            are stored here. If the parameters from different commands are
            not orthogonal (the order of the commands will cause the subarray
            to have a different internal state) then a timestamp must be
            included so that the commands can be played back in the
            correct order.
        </description>

         <Container name="Track">
            <description>
               Subarray track command.  This track container contains
               the source when track() is called with ants=0 (all antennas).
               This is the only source name that should appear in MIRIAD data
            </description>

            <MonitorPoint name="timestamp" type="absTime" persistent="true">
              <shortName>Timestamp</shortName>
              <units>UTC</units>
              <precision> 0 </precision>
              <description>Time this command was executed</description>
            </MonitorPoint>

            <MonitorPoint name="sourcename" type="string" persistent="true">
                <shortName>Trk Src</shortName>
                <longName>Sky tracking source</longName>
                <description>
                    The name of the source being tracked by antennas in this
                    subarray. This may be different than the source used for
                    Doppler tracking of the local oscillator.
                </description>
            </MonitorPoint>

          </Container> <!-- End track command -->

          <Container name="WindTrack">

            <description>Subarray wind track command. </description>

            <MonitorPoint name="timestamp" type="absTime" persistent="true">
              <shortName>Timestamp</shortName>
              <units>UTC</units>
              <precision> 0 </precision>
              <description>Time this command was executed</description>
            </MonitorPoint>
          </Container>

          <Container name="SnowTrack">

            <description>Subarray wind track command. </description>

            <MonitorPoint name="timestamp" type="absTime" persistent="true">
              <shortName>Timestamp</shortName>
              <units>UTC</units>
              <precision> 0 </precision>
              <description>Time this command was executed</description>
            </MonitorPoint>
          </Container>

          <CommonContainer ref="EquatOffset"/>
          <CommonContainer ref="Move"/>
          <CommonContainer ref="Stop"/>
          <CommonContainer ref="Stow"/>

          <Container name="PhaseCenterOffset" priority="vital">
            <description>
                Command to set the right ascension and declination
                offsets for the subarray phase tracking center.
            </description>

            <MonitorPoint name="timestamp" type="absTime" persistent="true">
              <shortName>Timestamp</shortName>
              <units>UTC</units>
              <precision> 0 </precision>
              <description>
                 Time that command was executed.
              </description>
            </MonitorPoint>

            <MonitorPoint name="ra" type="float" persistent="true" priority="vital">
                <shortName>dra</shortName>
                <longName>RaOffset
                </longName>
                <description>
                    Right ascension offset from nominal coordinate
                    for the phase center. As an arc on the sky, not
                    coordinate offset.  This is, for example, the RA
                    offset used in mosaicking, and corresponds to
                    the MIRIAD header variable 'dra'.
                </description>
                <units>arcmin</units>
                <precision>2</precision>
            </MonitorPoint>

            <MonitorPoint name="dec" type="float" persistent="true">
                <shortName>ddec</shortName>
                <longName>DecOffset
                </longName>
                <description>
                    Declination offset from nominal coordinate for
                    the phase center. This is, for example, the DEC
                    offset used in mosaicking, and corresponds to
                    the MIRIAD header variable 'ddec'.
                </description>
                <units>arcmin</units>
                <precision>2</precision>
            </MonitorPoint>
        </Container> <!-- End phaseCenterOffset command -->

        <Container name="Freq" >
            <description>
                Command to set the first LO
            </description>

            <MonitorPoint name="timestamp" type="absTime" persistent="true">
              <shortName>Timestamp</shortName>
              <units>UTC</units>
              <precision> 0 </precision>
              <description>
                 Time that command was executed.
              </description>
            </MonitorPoint>

            <MonitorPoint name="restFreq" type="double" persistent="true">
                <shortName>RestFreq</shortName>
                <longName>FirstLORestFrequency</longName>
                <description>
                    Rest frequency used to setup the first LO.
                </description>
                <units>GHz</units>
                <precision>6</precision>
            </MonitorPoint>

            <MonitorPoint name="ifFreq" type="double" persistent="true">
                <shortName>IFfreq</shortName>
                <longName>IFfrequency</longName>
                <description>
                    IF frequency that the rest frequency is mapped to after
                    it is Doppler shifted.
                </description>
                <units>GHz</units>
                <precision>6</precision>
            </MonitorPoint>

            <MonitorPoint name="sideband" type="enum" persistent="true">
                <shortName>Sideband</shortName>
                <longName>Sideband</longName>
                <description>
                    Sideband in which to place the rest frequency
                </description>
                <enum name="USB"/>
                  <description>Upper sideband</description>
                <enum name="LSB"/>
                  <description>Upper sideband</description>
            </MonitorPoint>

        </Container> <!-- End phaseCenterOffset command -->

      </Container>

<!-- END Subarray commands -->
<!-- ============================================================= -->
      <MonitorPoint name="lastCommand" type="string" persistent="true"
           priority="dontarchive" >
        <description>
             String representation of the last command issued to this subarray.
        </description>
      </MonitorPoint>

      <MonitorPoint name="comment" type="string" persistent="false"
           priority="vital" >
        <description>
             A comment input by the observer via the comment() command.
             This gets put into the MIRIAD history file.
             Note this monitor point is NOT persistent, making it
             easier to insert the monitor point into the history file
             at the correct time index.  If you need a persistent
             copy of this monitor point for display or query purposes, use
             Control.SubarrayN.notice.
        </description>
      </MonitorPoint>

<!-- NB could use this for lastCommand too? -->
      <MonitorPoint name="notice" type="string" persistent="true"
           priority="dontarchive" >
        <description>
             A persistent copy of the comment input by the observer via
             the comment() command that can be, e.g., displayed in RTD windows.
             This monitor point is for display/query only and is not archived.
        </description>
      </MonitorPoint>

