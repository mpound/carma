<!-- Thread Pool Statistics -->

      <Container name="ThreadStats">
        <description>
          Statistics about the WorkerPool threads running inside the
          SubarrayControl program.
        </description>

        <MonitorPoint name="frameExceptions" type="int">
          <description>
            Number of exceptions which happened during WorkRequest execution
            during this 500ms frame.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalExceptions" type="int">
          <description>
            Number of exceptions which happened during WorkRequest execution
            since the SubarrayControl process was started.
          </description>
        </MonitorPoint>

        <MonitorPoint name="instExecuteCount" type="int">
          <description>
            The instantaneous number of threads which are currently executing a
            WorkRequest. This is the number of threads which are currently busy.
          </description>
        </MonitorPoint>

        <MonitorPoint name="instQueueCount" type="int">
          <description>
            The instantaneous number of WorkRequest tasks which are queued for
            execution. This is the current length of the queue of pending tasks.
          </description>
        </MonitorPoint>

        <!-- per-frame statistics on queueing -->
        <MonitorPoint name="frameQueueCount" type="int">
          <description>
            The number of WorkRequest queued during this 500ms frame
          </description>
        </MonitorPoint>

        <MonitorPoint name="frameQueueTimeMaxId" type="string">
          <width>80</width>
          <description>
            The id of the WorkRequest which was present in the queue
            (enqueue until start-of-execution) for the maximum period of time
            during this 500ms frame.

            Note that this only applies to tasks which have moved into the
            start-of-execution state. Tasks which are still waiting in the queue
            do not effect this value.
          </description>
        </MonitorPoint>

        <MonitorPoint name="frameQueueTimeMax" type="int">
          <units>us</units>
          <description>
            The maximum number of microseconds a task was present in the queue
            (enqueue until start-of-execution) during this 500ms frame.

            Note that this only applies to tasks which have moved into the
            start-of-execution state. Tasks which are still waiting in the queue
            do not effect this value.
          </description>
        </MonitorPoint>

        <MonitorPoint name="frameQueueTimeMin" type="int">
          <units>us</units>
          <description>
            The minimum number of microseconds a task was present in the queue
            (enqueue until start-of-execution) during this 500ms frame.

            Note that this only applies to tasks which have moved into the
            start-of-execution state. Tasks which are still waiting in the queue
            do not effect this value.
          </description>
        </MonitorPoint>

        <MonitorPoint name="frameQueueTimeAvg" type="int">
          <units>us</units>
          <description>
            The average number of microseconds a task was present in the queue
            (enqueue until start-of-execution) during this 500ms frame.

            Note that this only applies to tasks which have moved into the
            start-of-execution state. Tasks which are still waiting in the queue
            do not effect this value.
          </description>
        </MonitorPoint>

        <!-- all-time statistics on queueing -->
        <MonitorPoint name="totalQueueCount" type="int">
          <description>
            The number of WorkRequest queued since the SubarrayControl process
            was started.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalQueueTimeMaxId" type="string">
          <width>80</width>
          <description>
            The id of the WorkRequest which was present in the queue
            (enqueue until start-of-execution) for the maximum period of time
            since the SubarrayControl process was started.

            Note that this only applies to tasks which have moved into the
            start-of-execution state. Tasks which are still waiting in the queue
            do not effect this value.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalQueueTimeMax" type="int">
          <units>us</units>
          <description>
            The maximum number of microseconds a task was present in the queue
            (enqueue until start-of-execution) since the SubarrayControl process
            was started.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalQueueTimeMin" type="int">
          <units>us</units>
          <description>
            The minimum number of microseconds a task was present in the queue
            (enqueue until start-of-execution) since the SubarrayControl process
            was started.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalQueueTimeAvg" type="int">
          <units>us</units>
          <description>
            The average number of microseconds a task was present in the queue
            (enqueue until start-of-execution) since the SubarrayControl process
            was started.
          </description>
        </MonitorPoint>

        <!-- per-frame statistics on execution -->
        <MonitorPoint name="frameExecuteCount" type="int">
          <description>
            The number of WorkRequest executed during this 500ms frame
          </description>
        </MonitorPoint>

        <MonitorPoint name="frameExecuteTimeMaxId" type="string">
          <width>80</width>
          <description>
            The id of the WorkRequest which was executing
            (start-of-execution until end-of-execution) for the maximum period
            of time during this 500ms frame.

            Note that this only applies to tasks which have moved into the
            end-of-execution state. Tasks which are still executing do not
            effect this value.
          </description>
        </MonitorPoint>

        <MonitorPoint name="frameExecuteTimeMax" type="int">
          <units>us</units>
          <description>
            The maximum number of microseconds a task was executing
            (start-of-execution until end-of-execution) during this 500ms frame.

            Note that this only applies to tasks which have moved into the
            end-of-execution state. Tasks which are still executing do not
            effect this value.
          </description>
        </MonitorPoint>

        <MonitorPoint name="frameExecuteTimeMin" type="int">
          <units>us</units>
          <description>
            The minimum number of microseconds a task was executing
            (start-of-execution until end-of-execution) during this 500ms frame.

            Note that this only applies to tasks which have moved into the
            end-of-execution state. Tasks which are still executing do not
            effect this value.
          </description>
        </MonitorPoint>

        <MonitorPoint name="frameExecuteTimeAvg" type="int">
          <units>us</units>
          <description>
            The average number of microseconds a task was executing
            (start-of-execution until end-of-execution) during this 500ms frame.

            Note that this only applies to tasks which have moved into the
            end-of-execution state. Tasks which are still executing do not
            effect this value.
          </description>
        </MonitorPoint>

        <!-- all-time statistics on execution -->
        <MonitorPoint name="totalExecuteCount" type="int">
          <description>
            The number of WorkRequest executed since the SubarrayControl process
            was started.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalExecuteTimeMaxId" type="string">
          <width>80</width>
          <description>
            The id of the WorkRequest which was executing
            (start-of-execution until end-of-execution) for the maximum period
            of time since the SubarrayControl process was started.

            Note that this only applies to tasks which have moved into the
            end-of-execution state. Tasks which are still executing do not
            effect this value.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalExecuteTimeMax" type="int">
          <units>us</units>
          <description>
            The maximum number of microseconds a task was executing
            (start-of-execution until end-of-execution) since the
            SubarrayControl process was started.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalExecuteTimeMin" type="int">
          <units>us</units>
          <description>
            The minimum number of microseconds a task was executing
            (start-of-execution until end-of-execution) since the
            SubarrayControl process was started.
          </description>
        </MonitorPoint>

        <MonitorPoint name="totalExecuteTimeAvg" type="int">
          <units>us</units>
          <description>
            The average number of microseconds a task was executing
            (start-of-execution until end-of-execution) since the
            SubarrayControl process was started.
          </description>
        </MonitorPoint>

      </Container>


