<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<!-- Monitor points for repeat tasks (RepTasks)                          -->
<!-- ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++  -->
<Container name="RepTask" count="&NUM_REP_TASKS;" >
    <description>
        Contains monitor points necessary for implementing repeat tasks
    </description>
    <MonitorPoint name="taskName" type="string" persistent="true">
        <shortName>TaskName</shortName>
        <longName>Task name</longName>
        <description>Name of the repetitive task</description>
        <width>16</width>
    </MonitorPoint>
    <MonitorPoint name="timeLastDone" type="absTime" persistent="true">
        <shortName>LastDone</shortName>
        <longName>Time last done</longName>
        <description>
         The last time the task was completed, as an MJD
        </description>
        <precision>0</precision>
        <width>16</width>
    </MonitorPoint>
    <MonitorPoint name="since" type="double" persistent="true">
        <shortName>Since</shortName>
        <longName>Time since last done</longName>
        <description>
            The time interval since the task was last completed
        </description>
        <units>days</units>
        <precision>3</precision>
    </MonitorPoint>
    <MonitorPoint name="repeatInterval" type="double" persistent="true">
        <shortName>RepeatInterval</shortName>
        <longName>Repeat interval</longName>
        <description>
                The nominal interval at which the task should be repeated. 
                This monitor point is advisory only and is not used by the code.
        </description>
        <units>days</units>
        <precision>2</precision>
        <width>5</width>
    </MonitorPoint>
    <MonitorPoint name="autoInterval" type="double" persistent="true">
        <shortName>AutoInterval</shortName>
        <longName>Auto interval</longName>
        <description>
         The time interval since the last run when the task should be
         automatically repeated. Note that only some tasks are automatically
         executed before this interval, usually as part of the queue program. 
         The rest need to be executed manually.
         This monitor point is advisory only and is not used by the code.
        </description>
        <units>days</units>
        <precision>2</precision>
        <width>5</width>
    </MonitorPoint>
    <MonitorPoint name="reminderInterval" type="double" persistent="true">
        <shortName>RemindInterval</shortName>
        <longName>Remind interval</longName>
        <description>
         The time interval since the last run that a reminder should be issued.
         This is usually an email, and sometimes an alarm. The observer should
         run the task as requested.
        </description>
        <units>days</units>
        <precision>2</precision>
        <width>5</width>
    </MonitorPoint>
    <MonitorPoint name="reminderReady" type="bool" persistent="true">
        <shortName>ReminderReady</shortName>
        <longName>Reminder Ready</longName>
        <description>
            Time to send a reminder.
            If this is done, it is done by the fault system and can be 
            an alarm or email. The fault system needs to be configured,
            using this monitor point as a trigger.
        </description>
        <errHi>true</errHi>
    </MonitorPoint>
    <MonitorPoint name="autoReady" type="bool" persistent="true">
        <shortName>AutoReady</shortName>
        <longName>Auto Ready</longName>
        <description>
            Time to automatically execute the task.
            Not all tasks are automatically executed, but this is the
            the MP to use to trigger those that are.
         </description>
        <errHi>true</errHi>
    </MonitorPoint>
</Container>
<MonitorPoint name="repTaskErrors" type="int" persistent="true">
    <shortName>RepTaskErrs</shortName>
    <longName>RepTask Errors</longName>
    <description>
        Counter for errors (exceptions thrown) when repetitive task times are
        updated in the subarray control monitor system pre-writer.
    </description>
    <errHi>true</errHi>
</MonitorPoint>

</Subsystem>
