<?xml version="1.0"?>
<!DOCTYPE Subsystem SYSTEM "mpml.dtd">

<Subsystem name="Ovro" count="6" indexName="antennaNo" priority="useful"
           author="Andy Beard">

   <?common AntennaCommon.mpml?>
   <?common AntennaIF.mpml?>
   <?common CanbusCommon.mpml?>
   <?common Encoder.mpml?>
   <?common GunnPll.mpml?>
   <?common LoReferenceMonitor.mpml?>
   <?common SisReceiver.mpml?>
   <?common TiltmeterCommon.mpml?>
   <?common TimestampCommon.mpml?>
   <?common Varactor.mpml?>
   <?common OnlineCommon.mpml?>

   <?common MonitorStats.mpml?>
   <CommonContainer ref="MonitorSubsystemStats" />

   <CommonMonitorPoint ref="timestamp" />
   <CommonMonitorPoint ref="online" />

   <CommonContainer ref="AntennaCommon" />

   <!-- The Ovro antennas optimally contain two busses each. -->
   <!-- This is a little trickery to easily display this info on RTD -->
   <Container name="Can">
        <CommonContainer ref="Host" />
        <CommonContainer ref="Bus" count="2" />
   </Container>

   <Container name="Cryo">


      <!-- ************************************************** -->
      <!-- CryoCompressor module monitor point definitions   -->
      <!-- ************************************************** -->
      <Container name="Compressor">
        <description>Cryo Compressor CAN module </description>

         <CommonMonitorPoint ref="state" />

         <!-- Blanking frame monitor packet 1 -->
         <MonitorPoint name="ambientTemp" type="float">
            <shortName>T (ambient)</shortName>
            <longName>Ambient Temperature</longName>
            <description>Ambient air temp underneath platform</description>
            <units>C</units>
            <precision>1</precision>
         </MonitorPoint>

         <MonitorPoint name="cabinetTemp" type="float">
            <shortName>T (cabinet)</shortName>
            <longName>Cabinet Temperature</longName>
            <units>C</units>
            <precision>1</precision>
            <description>Compressor environmental enclosure
                internal temperature</description>
         </MonitorPoint>

         <MonitorPoint name="compressorTemp" type="float">
            <shortName>T (compressor)</shortName>
            <longName>Compressor Temperature</longName>
            <units>C</units>
            <precision>1</precision>
            <description>Compressor cabinet internal temperature</description>
         </MonitorPoint>

         <MonitorPoint name="returnTemp" type="float">
            <shortName>T (return)</shortName>
            <longName>Return Temperature</longName>
            <units>C</units>
            <precision>1</precision>
            <description>Compressor return helium gas temperature</description>
         </MonitorPoint>

         <!-- Blanking frame monitor packet 2 -->
         <MonitorPoint name="highSidetemp" type="float">
            <shortName>T (high side)</shortName>
            <longName>High Side Temperature</longName>
            <units>C</units>
            <precision>1</precision>
            <description>
                Compressor high side helium gas temperature.
            </description>
            <errLo>5.0</errLo>
            <warnLo>10.0</warnLo>
            <warnHi>85.0</warnHi>
            <errHi>90.0</errHi>
         </MonitorPoint>

         <MonitorPoint name="supplyPressure" type="float">
            <shortName>P (supply)</shortName>
            <longName>Supply Pressure</longName>
            <units>PSI</units>
            <precision>1</precision>
            <description>Compressor high side helium gas pressure
            </description>
            <warnLo>200.0</warnLo>
            <warnHi>280.0</warnHi>
            <errLo>180.0</errLo>
            <errHi>300.0</errHi>
         </MonitorPoint>

         <MonitorPoint name="returnPressure" type="float">
            <shortName>P (return)</shortName>
            <longName>Return Pressure</longName>
            <units>PSI</units>
            <precision>1</precision>
            <description>Compressor return helium gas pressure
            </description>
         </MonitorPoint>

         <MonitorPoint name="outLouverCmd" type="float">
            <shortName>Out Louv Cmd</shortName>
            <units>%</units>
            <precision>2</precision>
            <description>Environmental enclosure outlet louver command voltage.
            </description>
         </MonitorPoint>

         <!-- Blanking frame monitor packet 3 -->
         <MonitorPoint name="inLouverCmd" type="float">
            <shortName>In Louv Cmd</shortName>
            <units>%</units>
            <precision>2</precision>
            <description>Environmental enclosure inlet louver command voltage
            </description>
         </MonitorPoint>

         <MonitorPoint name="outLouverPos" type="float">
            <shortName>Out Louv Pos</shortName>
            <units>%</units>
            <precision>2</precision>
            <description>Environmental enclosure outlet louver actual position
            return voltage </description>
         </MonitorPoint>

         <MonitorPoint name="inLouverPos" type="float">
            <shortName>In Louv Pos</shortName>
            <units>%</units>
            <precision>2</precision>
            <description>Environmental enclosure inlet louver actual position
            return voltage </description>
         </MonitorPoint>

         <!-- Blanking frame monitor packet 4 -->
         <MonitorPoint name="heCylinderPressure" type="float">
            <shortName>P (cylinder)</shortName>
            <units>PSI</units>
            <precision>1</precision>
            <description>Helium cylinder pressure </description>
         </MonitorPoint>

         <MonitorPoint name="highPressureAlarm" type="bool">
            <shortName>Pressure Alarm</shortName>
            <description>Compressor high pressure alarm</description>
            <warnHi>true</warnHi>
         </MonitorPoint>

         <MonitorPoint name="highTemperatureAlarm" type="bool">
            <shortName>Comp Temp Alarm</shortName>
            <description>Compressor high temperature alarm</description>
            <warnHi>true</warnHi>
         </MonitorPoint>

         <MonitorPoint name="roomTemperatureAlarm" type="bool">
            <shortName>Room Temp Alarm</shortName>
            <description>Compressor room temperature alarm</description>
            <warnHi>true</warnHi>
         </MonitorPoint>

         <MonitorPoint name="fridgeDriveState" type="enum">
            <shortName>Drive</shortName>
            <longName>Drive Status</longName>
            <description>Refrigerator drive on or off</description>
            <enum name="OFF"/> <!-- Ox00 -->
            <errHi>OFF</errHi>
            <enum name="ON"/>  <!-- 0x01 -->
         </MonitorPoint>

         <MonitorPoint name="tempError" type="float">
            <shortName>Temp Error</shortName>
            <longName>Temperature Error</longName>
            <units>C</units>
            <precision>2</precision>
            <description>
                Actual supply temperarature minus setpoint temperature.
            </description>
         </MonitorPoint>

         <MonitorPoint name="tempLoopState" type="enum">
            <shortName>Loop State</shortName>
            <description>State of temperature control loop for
            compressor cabinet </description>
            <enum name="DISABLED" /> <!-- 0x00 -->
            <enum name="ENABLED" />  <!-- 0x01 -->
         </MonitorPoint>

         <MonitorPoint name="moduleTemperature" type="float">
            <shortName>Mod Temp</shortName>
            <description>CAN module internal temperature</description>
            <precision>1</precision>
         </MonitorPoint>

         <!-- Blanking frame monitor packet 5 -->
         <MonitorPoint name="powerSupply24v" type="float">
            <shortName>+24V</shortName>
            <longName>Positive 24V Power Supply Voltage</longName>
            <units>V</units>
            <precision>1</precision>
            <description>24 volt power supply voltage</description>
            <warnLo>20.0</warnLo>
            <warnHi>28.0</warnHi>
            <errLo>18.0</errLo>
            <errHi>30.0</errHi>
         </MonitorPoint>

         <MonitorPoint name="powerSupply12v" type="float">
            <shortName>+12V</shortName>
            <longName>Positive 12V Power Supply Voltage</longName>
            <units>V</units>
            <precision>1</precision>
            <description>12 volt power supply voltage</description>
            <warnLo>11.5</warnLo>
            <warnHi>12.5</warnHi>
            <errLo>11.0</errLo>
            <errHi>13.0</errHi>
         </MonitorPoint>

         <MonitorPoint name="powerSupply5vAnalog" type="float">
            <shortName>+5V Analog</shortName>
            <units>V</units>
            <precision>1</precision>
            <description>5 volt analog power supply voltage</description>
            <warnLo>4.85</warnLo>
            <warnHi>5.15</warnHi>
            <errLo>4.75</errLo>
            <errHi>5.25</errHi>
         </MonitorPoint>

         <MonitorPoint name="powerSupply5vDigital" type="float">
            <shortName>+5V Digital</shortName>
            <units>V</units>
            <precision>1</precision>
            <description>5 volt digital power supply voltage</description>
            <warnLo>4.85</warnLo>
            <warnHi>5.15</warnHi>
            <errLo>4.75</errLo>
            <errHi>5.25</errHi>
         </MonitorPoint>


         <!-- System monitor packets -->

         <!-- Include common microprocessor monitor points -->
         <CommonContainer ref="Xac" />

      </Container>

      <!-- ************************************************** -->
      <!-- *********** End CryoCompressor device ************ -->
      <!-- ************************************************** -->

      <!-- ************************************************** -->
      <!-- CryoTemperature module monitor point definitions   -->
      <!-- ************************************************** -->
      <Container name="Dewar" >
        <description>Cryo temperatures CAN module</description>

         <CommonMonitorPoint ref="state" />

         <MonitorPoint name="firstStageTemp" type="float">
            <shortName>1st Stage Temp</shortName>
            <units>K</units>
            <description>Cryostat first stage temperature</description>
            <precision>2</precision>
         </MonitorPoint>

         <MonitorPoint name="shieldTemp" type="float">
            <shortName>Shield Temp</shortName>
            <units>K</units>
            <description>1st Stage Shield Temperature</description>
            <precision>2</precision>
         </MonitorPoint>

         <MonitorPoint name="heliumPotTemp" type="float">
            <shortName>He Pot Temp</shortName>
            <units>K</units>
            <description>Helium Pot Temperature</description>
            <precision>2</precision>
         </MonitorPoint>

         <MonitorPoint name="plate4kTemp" type="float">
            <shortName>4K Plate Temp</shortName>
            <units>K</units>
            <description>Four Kelvin Plate Temperature</description>
            <precision>2</precision>
            <warnLo>3.5</warnLo>
            <warnHi>5.5</warnHi>
            <errLo>3.0</errLo>
            <errHi>6.5</errHi>
         </MonitorPoint>

         <MonitorPoint name="hastingsVac" type="float">
            <shortName>Hastings Vac</shortName>
            <units>mtorr</units>
            <description>Cryostat vacuum, Hastings gauge</description>
            <precision>2</precision>
         </MonitorPoint>

         <MonitorPoint name="modionVac" type="float">
            <shortName>Modion Vac</shortName>
            <units>utorr</units>
            <description>Cryostat vacuum, Modion gauge</description>
            <precision>2</precision>
         </MonitorPoint>

         <MonitorPoint name="ps24v" type="float">
            <shortName>+24V</shortName>
            <longName>24V Power Supply</longName>
            <units>V</units>
            <description>+24 V power supply voltage </description>
            <precision>2</precision>
            <warnLo>20.0</warnLo>
            <warnHi>28.0</warnHi>
            <errLo>18.0</errLo>
            <errHi>30.0</errHi>
         </MonitorPoint>

         <MonitorPoint name="ps12v" type="float">
            <shortName>+12V</shortName>
            <longName>12V Power Supply</longName>
            <units>V</units>
            <description>+12 V power supply voltage </description>
            <precision>2</precision>
            <warnLo>11.5</warnLo>
            <warnHi>12.5</warnHi>
            <errLo>11.0</errLo>
            <errHi>13.0</errHi>
         </MonitorPoint>

         <MonitorPoint name="ps5vAnalog" type="float">
            <shortName>+5V Analog</shortName>
            <units>V</units>
            <description>+5 V analog power supply voltage </description>
            <precision>2</precision>
            <warnLo>4.85</warnLo>
            <warnHi>5.15</warnHi>
            <errLo>4.75</errLo>
            <errHi>5.25</errHi>
         </MonitorPoint>

         <MonitorPoint name="ps5vDigital" type="float">
            <shortName>+5V Digital</shortName>
            <units>V</units>
            <description>+5 V digital power supply voltage </description>
            <precision>2</precision>
         </MonitorPoint>

         <!-- System monitor packets -->

         <!-- Include common microprocessor monitor points -->
         <CommonContainer ref="Xac" />
      </Container>

      <!-- End CryoTemperature module monitor point definitions -->

   </Container>

   <!-- End Cryo Container -->

    <Container name="Frontend">
    </Container>

    <!-- Drive Container -->
    <Container name="Drive" priority="vital">
        <MonitorPoint name="sourcename" type="string">
            <shortName>Source</shortName>
            <longName>Sourcename</longName>
            <description>
                Source name for ra/dec
            </description>
            <width>8</width>
        </MonitorPoint>
        <MonitorPoint name="rightAscension" type="double">
            <shortName>RA</shortName>
            <longName>RightAscension</longName>
            <description>
               Apparent right ascension
            </description>
            <units>rad</units>
        </MonitorPoint>
        <MonitorPoint name="declination" type="double">
            <shortName>Dec</shortName>
            <longName>Declination</longName>
            <description>
             Apparent declination
            </description>
            <units>rad</units>
        </MonitorPoint>
        <MonitorPoint name="errorSky" type="float">
            <shortName>SkyErr</shortName>
            <longName>ErrorSky</longName>
            <description>
                Total tracking error on the sky
            </description>
            <precision>1</precision>
            <units>arcsec</units>
        </MonitorPoint>
        <!-- These enums must agree exactly with AntennaCommon -->
        <MonitorPoint name="driveState" type="enum">
            <shortName>DriveState</shortName>
            <longName>Drive State</longName>
            <description>
              The state of the drive system
            </description>
            <enum name="TRACK">
              <description> Tracking</description>
            </enum>
            <enum name="CLOSE">
              <description>
                Close to the source - the antenna is no longer slewing but is
                trying to track the source within tolerance. If the antenna is
                in TRACK and then falls outside the tolerance, it will go into
                this state.
              </description>
            </enum>
            <enum name="SLEW">
              <description>Slewing</description>
            </enum>
            <enum name="STOW">
              <description>Currently in stow position</description>
            </enum>
            <enum name="SNOW">
              <description>Currently minimizing snow buildup</description>
            </enum>
            <enum name="STOP">
              <description>Stopped, drive power off</description>
            </enum>
            <enum name="DISABLE">
              <description>
                Manually disabled at the antenna, or collision detected.
              </description>
            </enum>
            <enum name="HWLIMIT">
                <description>Hardware limit</description>
            </enum>
            <enum name="SWLIMIT">
                <description>Software limit</description>
            </enum>
            <enum name="ERROR">
                <description>Error state (stalled/power problem)</description>
            </enum>
            <enum name="LOCAL">
                <description>
                    Antenna is under local control (i.e. handpaddle).
                </description>
            </enum>
            <enum name="TEST">
                <description>
                    Drive system is in a test mode.
                </description>
            </enum>
            <enum name="FATAL">
                <description>
                    A fatal drive system error has been detected.
                    These errors include an az overlap switch failure,
                    an impossible rate of motion in either axis, 
                    an illegal azimuth podition, or a drive controller 
                    overtemp.
                </description>
            </enum>

            <errHi>ERROR</errHi>
            <errHi>HWLIMIT</errHi>
            <errHi>DISABLE</errHi>
            <warnHi>SWLIMIT</warnHi>
            <errHi>LOCAL</errHi>
            <errHi>TEST</errHi>
            <errHi>FATAL</errHi>
        </MonitorPoint>

        <!-- These enums must agree exactly with AntennaCommon -->
        <MonitorPoint name="driveMode" type="enum">
            <shortName>DriveMode</shortName>
            <longName>Drive Control Mode</longName>
            <description> Commanded mode of drive system </description>
             <enum name="AZEL" >
              <description>
                Horizon coordinate system (azimuth and elevation)
              </description>
             </enum>
             <enum name="EQUAT" >
              <description>Equatorial coordinate system (ra/dec)</description>
             </enum>
             <enum name="STOW" >
              <description>Stow</description>
             </enum>
             <enum name="SNOW">
              <description>Minimize snow buildup on antennas</description>
             </enum>
             <enum name="STOP" >
              <description>Stopped, with drive system turned off</description>
             </enum>
             <enum name="TEST" >
              <description>
                Drive system is in testing mode for engineering purposes.
                This may be because of an open servo loop or becaues the
                drive module has been placed in engineering mode.
              </description>
             </enum>
             <errHi>TEST</errHi>
        </MonitorPoint>

        <MonitorPoint name="sequenceNo" type="int">
            <shortName>SeqNo</shortName>
            <longName>Sequene Number</longName>
            <description>
                Sequence number of last completed drive command.
            </description>
        </MonitorPoint>

        <MonitorPoint name="sequenceNoSuccess" type="bool" >
            <shortName>SeqNoSuccess</shortName>
            <longName>Sequence Number Success</longName>
            <description>
                Indicates if the command associated with the last sequence
                number was successful.
            </description>
            <errHi>false</errHi>
        </MonitorPoint>

        <Container name="Track" >
            <MonitorPoint name="requestedAzimuth" type="double">
                <shortName>ReqAz</shortName>
                <longName>RequestedAzimuth</longName>
                <description>
                    Requested azimuth; north=0.  This is the total requested
                    azimuth including offsets.
                </description>
                <precision>3</precision>
                <units>deg</units>
            </MonitorPoint>
            <MonitorPoint name="actualAzimuth" type="double">
                <shortName>ActAz</shortName>
                <longName>Actual azimuth</longName>
                <description> Actual azimuth; north=0 </description>
                <precision>3</precision>
                <units>deg</units>
            </MonitorPoint>
            <MonitorPoint name="errorAzimuth" type="float">
                <shortName>AzErr</shortName>
                <longName>ErrorAzimuth</longName>
                <description>
                    Error between the actual and requested azimuth, on encoder
                </description>
                <precision>1</precision>
                <units>arcsec</units>
            </MonitorPoint>
            <MonitorPoint name="errorAzimuthSky" type="float">
                <shortName>AzSkyErr</shortName>
                <longName>ErrorAzimuthSky</longName>
                <description>
                    Error between the actual and requested azimuth, on the sky
                </description>
                <precision>1</precision>
                <units>arcsec</units>
            </MonitorPoint>

            <MonitorPoint name="rmsErrorAzimuth" type="float">
                <shortName>AzRMSErr</shortName>
                <longName>ErrorAzimuthRMS</longName>
                <description>
                    Azimuth error RMS over last twenty frames. Calculated
                    with respect to zero, not the mean error.
                </description>
                <precision>2</precision>
                <units>arcsec</units>
            </MonitorPoint>

            <MonitorPoint name="requestedElevation" type="double">
                <shortName>ReqEl</shortName>
                <longName>RequestedElevation</longName>
                <description>
                    Requested elevation; horizon=0. This is the total requested
                    elevation, including offsets.
                </description>
                <precision>3</precision>
                <units>deg</units>
            </MonitorPoint>
            <MonitorPoint name="actualElevation" type="double">
                <shortName>ActEl</shortName>
                <longName>Actual elevation</longName>
                <description> Actual elevation; horizon=0 </description>
                <precision>3</precision>
                <units>deg</units>
            </MonitorPoint>
            <MonitorPoint name="errorElevation" type="float">
                <shortName>ElErr</shortName>
                <longName>ErrorElevation</longName>
                <description>
                    Error between the requested and actual elevation
                </description>
                <precision>1</precision>
                <units>arcsec</units>
            </MonitorPoint>
            <MonitorPoint name="rmsErrorElevation" type="float">
                <shortName>ElRMSErr</shortName>
                <longName>ErrorElevationRMS</longName>
                <description>
                    Elevation error RMS over last twenty frames. Calculated
                    with respect to zero, not the mean error.
                </description>
                <precision>2</precision>
                <units>arcsec</units>
            </MonitorPoint>

            <MonitorPoint name="azimuthRate" type="float">
                <shortName>AzRate</shortName>
                <longName>AzimuthRate</longName>
                <description>
                    Azimuth rate (on the encoder, not sky)
                </description>
                <precision>3</precision>
                <units>deg/min</units>
            </MonitorPoint>
            <MonitorPoint name="elevationRate" type="float">
                <shortName>ElRate</shortName>
                <longName>ElevationRate</longName>
                <description>
                    Elevation rate
                </description>
                <precision>3</precision>
                <units>deg/min</units>
            </MonitorPoint>
            <MonitorPoint name="azEastCurrent" type="float">
                <shortName>azEastI</shortName>
                <longName>EastAzimuthMotorCurrent</longName>
                <precision>1</precision>
                <units>A</units>
                <description>East azimuth drive motor current</description>
            </MonitorPoint>
            <MonitorPoint name="azWestCurrent" type="float">
                <shortName>azWestI</shortName>
                <longName>WestAzimuthMotorCurrent</longName>
                <precision>1</precision>
                <units>A</units>
                <description>West azimuth drive motor current</description>
            </MonitorPoint>
            <MonitorPoint name="azPreloadCurrent" type="float">
                <shortName>preloadI</shortName>
                <longName>PreloadCurrent</longName>
                <precision>1</precision>
                <units>A</units>
                <description>Azimuth drive motors current difference</description>
            </MonitorPoint>
            <MonitorPoint name="elCurrent" type="float">
                <shortName>ElI</shortName>
                <longName>ElevationMotorCurrent</longName>
                <precision>1</precision>
                <units>A</units>
                <description>Elevation drive motor current</description>
            </MonitorPoint>
            <MonitorPoint name="wrapLogic" type="enum">
              <shortName>wrapLogic</shortName>
              <longName>Wrap Logic</longName>
              <description>
                The azimuth drive can operate over more than a full circle,
                and the wrap logic dictates how the ambiguities are resolved.
                Direction are defined as viewed from above.
              </description>
              <enum name="ZERO">
               <description>
                 Do not modify target position (no turns added).
               </description>
              </enum>
              <enum name="ADD">
               <description>
                 Add a turn to the target position (in AZ).
               </description>
              </enum>
              <enum name="SUB">
               <description>
                Subtract a turn to the target position (in AZ).
               </description>
              </enum>
            </MonitorPoint>
            <MonitorPoint name="limit" type="enum">
                <shortName>HwSwLimit</shortName>
                <longName>HwSwLimit</longName>
                <description>Hardware and/or software limits</description>
                <enum name="OK">
                 <description>No limits</description>
                </enum>
                <enum name="HW">
                 <description>Hardware limit detected</description>
                </enum>
                <enum name="SW">
                 <description>Software limit detected</description>
                </enum>
                <enum name="HWSW">
                 <description>A hardware and software limit has
                   has been exceeded on different axes
                 </description>
                </enum>
            </MonitorPoint>
            <MonitorPoint name="emergencyOff" type="enum">
                <shortName>EmergOff</shortName>
                <longName>EmergencyOff</longName>
                <description>
                    Drive controllers disabled by the emergency off
                    switch in the sidecab.
                </description>
                <enum name="OK">
                 <description>Drive not disabled</description>
                </enum>
                <enum name="SIDECAB">
                 <description>
                    Drives disabled by emergency off switch in sidecab
                 </description>
                </enum>
                <errHi>SIDECAB</errHi>
             </MonitorPoint>
             <MonitorPoint name="disabled" type="enum">
                <shortName>Disable</shortName>
                <longName>Disabled</longName>
                <description>
                    Drive controllers disabled by the local/remote/off switch in
                    the base of the mount (teepee).
                </description>
                <enum name="OK">
                 <description>Drive not disabled</description>
                </enum>
                <enum name="TEEPEE">
                 <description>
                    Drives disabled by local/remote/off switch in mount base
                 </description>
                </enum>
                <errHi>TEEPEE</errHi>
            </MonitorPoint>
            <MonitorPoint name="controllerFault" type="enum">
                <shortName>Fault</shortName>
                <longName>ControllerFault</longName>
                <description>Drive controllers fault states</description>
                <enum name="OK">
                 <description>No drive faults</description>
                </enum>
                <enum name="AZ">
                 <description>Azimuth drive controller fault</description>
                </enum>
                <enum name="EL">
                 <description>Elevation drive controller fault</description>
                </enum>
                <enum name="AZEL">
                 <description>Az and el drive controller faults</description>
                </enum>
                <errHi>AZ</errHi>
                <errHi>EL</errHi>
                <errHi>AZEL</errHi>
            </MonitorPoint>

        </Container>

        <Container name="Limit" >
            <description>
                Hardware and software limit conditions
            </description>
            <MonitorPoint name="azSwLimit" type="enum">
                <shortName>AzSwLimit</shortName>
                <longName>AzimuthSoftwareLimit</longName>
                <description>Azimuth software limits</description>
                <enum name="OK">
                 <description>No limits</description>
                </enum>
                <enum name="PLUSLIM">
                 <description>
                    Software azimuth limit exceeded in + direction
                 </description>
                </enum>
                <enum name="MINUSLIM">
                 <description>
                    Software azimuth limit exceeded in - direction
                 </description>
                </enum>
                <enum name="ERROR">
                 <description>
                    Hardware limit status could not be correctly determined.
                 </description>
                </enum>
                <errHi>PLUSLIM</errHi>
                <errHi>MINUSLIM</errHi>
