    <!-- ********* Rx Temperature Control Module ********** -->
    <!-- ************************************************** -->
    <Container name="RxThermalControl">
        <description>
            The 10-m Electronics Temperature Controller CAN module controls
            and monitors the temperature of the electronics modules in the
            thermal enclosure on the receiver plate.  There are two control
            loops, one for each end of the component mounting plate.  Loop 1
            is at the left end of the plate and loop 2 is at the right end when
            viewing the receiver plate with the antenna at zenith.
            Temperatures are sensed using DS18B20 1-wire thermometers and
            the temperature is controlled by applying current to power
            resistors on the plate.  The currents are generated with a pulse
            width modulated (PWM) H-bridge for high efficiency.
        </description>

        <!-- Common state monitor point -->
        <CommonMonitorPoint ref="state" />

        <!-- Blanking frame monitor packet 1 -->
        <MonitorPoint name="loopTemp" type="float" count="2">
            <shortName>LoopTemp</shortName>
            <longName>Loop Temperature</longName>
            <description>
                Temperature read by loop thermometer.
            </description>
            <units>C</units>
        </MonitorPoint>

        <MonitorPoint name="airTemp" type="float" >
            <shortName>AirTemp</shortName>
            <longName>Enclosure Air Temperature</longName>
            <description>
                Temperature measured inside the thermal enclosure.
            </description>
            <units>C</units>
        </MonitorPoint>

        <MonitorPoint name="moduleTemp" type="float" >
            <shortName>ModuleTemp</shortName>
            <longName>Module Temperature</longName>
            <description>
                Temperature of the CAN module.
            </description>
            <units>C</units>
        </MonitorPoint>

        <!-- Blanking frame monitor packet 2 -->
        <MonitorPoint name="heaterVoltage" type="float" count="2" >
            <shortName>HeaterV</shortName>
            <longName>Heater Voltage</longName>
            <description>
                Voltage applied to loop heater.
            </description>
            <units>V</units>
        </MonitorPoint>

        <MonitorPoint name="heaterCurrent" type="float" count="2" >
            <shortName>HeaterI</shortName>
            <longName>Heater Current</longName>
            <description>
                Current applied to loop heater.
            </description>
            <units>A</units>
        </MonitorPoint>

        <!-- Blanking frame monitor packet 3 & 5 -->
        <MonitorPoint name="heaterLoopState" type="enum" count="2" >
            <shortName>LoopState</shortName>
            <longName>Loop State</longName>
            <description>
                Status of the internal temperature feedback loop.
            </description>
            <enum name="HEAT_OFF" >
                <description>Heater off.</description>
            </enum>
            <enum name="NOMINAL" >
                <description>Loop error less than 0.1C.</description>
            </enum>
            <enum name="HI_BAD_RANGE" >
                <description>
                    Temperature too high, heater out of range.
                </description>
            </enum>
            <enum name="HI_IN_RANGE" >
                <description>
                    Temperature too high, heater in range.
                </description>
            </enum>
            <enum name="LOW_BAD_RANGE" >
                <description>
                    Temperature too low, heater out of range.
                </description>
            </enum>
            <enum name="LOW_IN_RANGE" >
                <description>
                    Temperature too low, heater in range.
                </description>
            </enum>
            <enum name="MANUAL" >
                <description>
                    Manual PWM mode.
                </description>
            </enum>
        </MonitorPoint>

        <MonitorPoint name="powerFraction" type="float" count="2" >
            <shortName>Power Frac</shortName>
            <longName>Power Fraction</longName>
            <description>
                Fraction of maximum power as a percentage.
            </description>
            <units>%</units>
        </MonitorPoint>

        <MonitorPoint name="tempDiff" type="float" count="2" >
            <shortName>TempDiff</shortName>
            <longName>Temperature Difference</longName>
            <description>
                Difference between set point and actual temperature.
            </description>
            <units>K</units>
        </MonitorPoint>

        <MonitorPoint name="integratedDiff" type="float" count="2" >
            <shortName>IntegDiff</shortName>
            <longName>Integrated Temperature Difference</longName>
            <description>
                Integrated difference between set point and actual temperature.
            </description>
            <units>K s</units>
        </MonitorPoint>

        <!-- Blanking frame monitor packet 4  & 6 -->
        <MonitorPoint name="heaterLoopGain" type="float" count="2" >
            <shortName>LoopGain</shortName>
            <longName>Heater Loop Gain</longName>
            <description>Loop gain.</description>
            <units>% max pwr per K</units>
        </MonitorPoint>

        <MonitorPoint name="integConstant" type="float" count="2" >
            <shortName>LoopIntegC</shortName>
            <longName>Loop Integration Constant</longName>
            <description>
                Loop gain for integrator.
            </description>
            <units>% max pwr K per s</units>
        </MonitorPoint>

        <MonitorPoint name="rateConstant" type="float" count="2" >
            <shortName>LoopRateC</shortName>
            <longName>Loop Rate Constant</longName>
            <description>Loop rate constant.</description>
            <units>% max pwr s per K</units>
        </MonitorPoint>

        <MonitorPoint name="bandwidth" type="float" count="2" >
            <shortName>LoopBW</shortName>
            <longName>Loop Bandwidth</longName>
            <description>Loop bandwidth.</description>
            <units>Hz</units>
        </MonitorPoint>

        <!-- Blanking frame monitor packet 7 -->
        <MonitorPoint name="ps24v" type="float" >
            <shortName>+24V</shortName>
            <longName>24V Power Supply Voltage</longName>
            <description>24V power supply voltage.</description>
            <units>V</units>
            <warnLo>20.0</warnLo>
            <warnHi>28.0</warnHi>
            <errLo>18.0</errLo>
            <errHi>30.0</errHi>
        </MonitorPoint>

        <MonitorPoint name="psPos12v" type="float" >
            <shortName>+12V</shortName>
            <longName>+12V Power Supply Voltage</longName>
            <description>
                +12V thermal control power supply voltage.
            </description>
            <units>V</units>
            <warnLo>11.5</warnLo>
            <warnHi>12.5</warnHi>
            <errLo>11.0</errLo>
            <errHi>13.0</errHi>
        </MonitorPoint>

        <MonitorPoint name="psNeg12v" type="float" >
            <shortName>-12V</shortName>
            <longName>-12V Power Supply Voltage</longName>
            <description>
                -12V thermal control power supply voltage.
            </description>
            <units>V</units>
            <warnLo>-12.5</warnLo>
            <warnHi>-11.5</warnHi>
            <errLo>-13.0</errLo>
            <errHi>-11.0</errHi>
        </MonitorPoint>

        <MonitorPoint name="ps5v" type="float" >
            <shortName>+5V</shortName>
            <longName>5V Power Supply Voltage</longName>
            <description>
                5V thermal control power supply voltage.
            </description>
            <units>V</units>
            <warnLo>4.85</warnLo>
            <warnHi>5.15</warnHi>
            <errLo>4.75</errLo>
            <errHi>5.25</errHi>
        </MonitorPoint>

        <CommonContainer ref="Xac"/>

    </Container>
