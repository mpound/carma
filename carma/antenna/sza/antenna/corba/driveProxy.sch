#------------------------------------------------------------
# stow() command
#------------------------------------------------------------

track zenith
until $elapsed > 1s

#------------------------------------------------------------
# trackSnow() command
#------------------------------------------------------------

track horizon
until $elapsed > 1s

#------------------------------------------------------------
# trackWind() command
#------------------------------------------------------------

track zenith
until $elapsed > 1s

#------------------------------------------------------------
# setAntLocation() command
#------------------------------------------------------------

site -118:08:29.91261, 37:16:49.36981, 2196.265
until $elapsed > 1s

#------------------------------------------------------------
# Select aperture
#------------------------------------------------------------

model optical
until $elapsed > 1s

#------------------------------------------------------------
# slew
#------------------------------------------------------------

slew az=180, el=45
until $elapsed > 1s

#------------------------------------------------------------
# collimate
#------------------------------------------------------------

collimate optical, 10, 20

#------------------------------------------------------------
# stop()
#------------------------------------------------------------

halt

