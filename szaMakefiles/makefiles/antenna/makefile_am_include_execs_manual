#######################################################################
# List of all programs
#######################################################################

antenna_sza_bins = bin/szaAntennaControl bin/szaMpmlGenerator bin/szaTestRemoteSensors bin/tRrdCollector bin/systemStatus bin/tRxProxy bin/tCal bin/tCmss bin/tCcss bin/tCmsr bin/tCmsa bin/tCarmaConfig bin/tReadArc bin/tPgplotTest bin/tAntProxy bin/tViewerTest bin/fluxcalLst bin/tRtdTest bin/tRtdClient bin/tRtdClientTask bin/tRtdServerTask bin/tPdb bin/tSort bin/tFastPdb bin/tCarmaArchiveFetcher bin/tTable bin/tVideoCapture bin/tFastPdbServer bin/tFastPdbClient

bin_PROGRAMS   += $(antenna_sza_bins)

#######################################################################
# Generic defines for all SZA executables
#######################################################################

antenna_sza_bins_ldadd = \
	$(lib_libcarmaSzaarrayutils_la) \
	$(lib_libcarmaSzautil_la) \
	$(lib_libcarmaSzaslalib_la) \
	$(lib_libcarmaSzaCfitsio_la) \
	$(lib_libcarmaSzaAntennaCanbus_la) \
	$(lib_libcarmaSzaAntennaControl_la) \
	$(lib_libcarmaSzaAntennaCorba_la) \
	$(antenna_common_la) \
	$(antenna_sza_la) \
	$(dbms_la) \
	$(monitor_la) \
	$(services_la) \
	$(environment_la) \
	$(correlator_la) \
	$(pipeline_la) \
	$(signalpath_la) \
	$(util_la) \
	-llog4cpp \
	-ljanz \
	-lmir \
	-lmysqlclient \
	-lodbc \
	-lxerces-c \
	-lnovas \
	-lgslcblas \
	-lgsl \
	-lunits \
	-lfftw3 \
	-lACE \
	-lTAO \
	-lTAO_AnyTypeCode \
	-lTAO_AnyTypeCode \
	-lTAO_CosNaming \
	-lTAO_CosNotification \
	-lTAO_ObjRefTemplate \
	-lTAO_PortableServer \
	-lTAO_CosNotification_Serv \
	-lTAO_CosNotification_MC \
	-lTAO_Utils \
	-ldl \
	-lreadline \
	-lcurl \
	-lrt \
	-lX11

antenna_sza_bins_cppflags = $(AM_CPPFLAGS) -DTOP="\"$(TOP)"\" -DCARMA=$(CARMA) -D_REENTRANT -D_GPP -DHAVE_CARMA=1 -DHAVE_CORBA=1 -DHAVE_CORR=0 -DHAVE_DC=0 -DHAVE_DELAY=0 -DANT_CORBA=1 -DCOMPILE_WITH_DEBUG=0 -DHAVE_SZA=0 -DHAVE_WX=0 -DIS_STABLE=0 -DCOMPILE_FOR_64BIT=0  -DMAC_OSX=0 -DHAVE_RT=1 -DHAVE_MATLAB=1 -DHAVE_VIDEO=1 -fPIC -Wno-deprecated

antenna_sza_bins_cxxflags = $(AM_CXXFLAGS) -fPIC

#######################################################################
# szaAntennaControl
#######################################################################

bin_szaAntennaControl_SOURCES = \
	carma/antenna/sza/executables/szaAntennaControl.cc
bin_szaAntennaControl_LDADD    = $(antenna_sza_bins_ldadd)
bin_szaAntennaControl_CPPFLAGS = $(antenna_sza_bins_cppflags)
bin_szaAntennaControl_CXXFLAGS = $(antenna_sza_bins_cxxflags)

