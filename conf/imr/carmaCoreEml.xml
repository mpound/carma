<!-- For inclusion by carma.xml only - this is not separately valid XML -->

    <!-- The main system 'acc' -->
    <oad hostname="&ACC;">
        <server name="delayEngineFSP" path="&BIN_DIR;/frameScriberPublisher"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG; &USE_DB; subsystem=delayEngine delay=250"
                startup-priority="&monitor;" />
        <server name="FrameCollator" path="&BIN_DIR;/frameCollator"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="traceFile=syslog traceLevel=0 &USE_DB; delay=600" 
                optargs="-ORBSvcConf &TOP_DIR;/conf/tao/fc.conf"
                system="monitor" critical="true" 
                startup-priority="&monitor-core;" />
        <server name="controlFSP" path="&BIN_DIR;/frameScriberPublisher"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG; &USE_DB;  subsystem=control delay=190"
                system="control" critical="true" 
                startup-priority="&monitor;"/>

        <server name="Subarray1" path="&BIN_DIR;/subarrayControl"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="subarrayNo=1 runSAT=true &LOG; &USE_DB; &SCRIPT_STATE;"
                optargs="-ORBSvcConf &TOP_DIR;/conf/tao/fc.conf"
                system="control" critical="true" 
                startup-priority="&control;"/>

        <server name="Subarray2" path="&BIN_DIR;/subarrayControl"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="subarrayNo=2 runSAT=true &LOG; &USE_DB; &SCRIPT_STATE;" 
                optargs="-ORBSvcConf &TOP_DIR;/conf/tao/fc.conf"
                system="control" critical="true"
                startup-priority="&control;"/>

        <server name="Subarray3" path="&BIN_DIR;/subarrayControl"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="subarrayNo=3 runSAT=true &LOG; &USE_DB; &SCRIPT_STATE;"
                optargs="-ORBSvcConf &TOP_DIR;/conf/tao/fc.conf"
                system="control" 
                startup-priority="&control;"/>

        <server name="Subarray4" path="&BIN_DIR;/subarrayControl"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="subarrayNo=4 runSAT=true &LOG; &USE_DB; &SCRIPT_STATE;"
                optargs="-ORBSvcConf &TOP_DIR;/conf/tao/fc.conf"
                system="control" startup-priority="&control;" />

        <server name="FaultSystem" path="&BIN_DIR;/faultSystem"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="&LOG; &USE_DB; runEmailService=true"
                startup-priority="&monitor-core;"/>

        <server name="PipelineSync" path="&BIN_DIR;/monitorPipelineSync"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG; &USE_DB;"
                system="monitor" critical="true" 
                startup-priority="&monitor-core;"/>

        <server name="MonitorAvgWriter" path="&BIN_DIR;/monitorAverageWriter"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG; &USE_DB; dbconffile=dbms/dbms.remote.conf framepriority=useful noframe=true" 
                system="monitor" critical="true" />
        
        <server name="ControlState" path="&BIN_DIR;/carmaControlState"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="statedir=/tmp/eml/stateTmp production=true &LOG; &USE_DB; nice=10"
                optargs="-ORBSvcConf &TOP_DIR;/conf/tao/fc.conf"
                system="control" critical="true" />

        <server name="ProcessMonitorFSP" path="&BIN_DIR;/frameScriberPublisher"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="&LOG; &USE_DB; subsystem=Imr delay=150"
                system="monitor" critical="true" 
                startup-priority="&monitor;"/>

        <server name="ProcessMonitor" path="&BIN_DIR;/processMonitor"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG; &USE_DB; file=/opt/rt/conf/imr/carma.xml writedelay=0.100 poll=5"
                system="monitor" critical="true" 
                startup-priority="&monitor;" />

        <server name="AstroFSP" path="&BIN_DIR;/frameScriberPublisher"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG;  &USE_DB; subsystem=astro delay=400" 
                system="data" 
                startup-priority="&monitor;"/>

        <server name="SlPipelineFSP" path="&BIN_DIR;/frameScriberPublisher"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG;  &USE_DB; subsystem=slpipeline delay=400" 
                system="data" 
                startup-priority="&monitor;"/>

        <server name="SlDataCatcher" path="&BIN_DIR;/catchData"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="&LOG;  &USE_DB; mode=sl" 
                optargs="-ORBSvcConf &TOP_DIR;/conf/tao/fc.conf"
                system="data" critical="true" 
                startup-priority="&subsystem;"/>                

        <server name="SlPipeline" path="&BIN_DIR;/pipeline"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="&LOG; &USE_DB;  mode=sl vbdir=/opt/visbrick monitorDelay=375" 
                system="data" critical="true"
                startup-priority="&subsystem;"/>
                
        <!-- Note sdpFiller requires pdbmHost to be up and running.
             So sdpFiller should be the last entry on sdp -->

        <!-- System status collector should be run on acc, since it accesses RRDs on that machine.
             (Note that it doesn't have to be run on acc, but if it isn't it will make many ssh 
             connections to acc) -->

        <server name="systemStatusFSP" path="&BIN_DIR;/frameScriberPublisher"
                directory="&DEFAULT_DIR;" mode="&STATE;" 
                args="&LOG; &USE_DB; subsystem=systemStatus"
                startup-priority="&monitor;"/>

        <server name="SystemStatus" path="&BIN_DIR;/systemStatus"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG;"
                startup-priority="&subsystem;"/>

        <server name="signalPathFSP" path="&BIN_DIR;/frameScriberPublisher"
	        directory="&DEFAULT_DIR;" mode="&STATE;"
	        args="&LOG; &USE_DB;  subsystem=signalpath delay=350"
            startup-priority="&monitor;" />
    
        <server name="SignalPathMapper" path="&BIN_DIR;/carmaSignalPathMapper"
	        directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG;"
                startup-priority="&subsystem;"/>

        <server name="CorrDataRemapper" 
                path="&BIN_DIR;/carmaCorrelatorDataRemapper"
                directory="&DEFAULT_DIR;" mode="&STATE;"
                args="&LOG;"
                startup-priority="&subsystem-core;"
                system="data"
                critical="true" />

        <!-- Wideband components of data pipeline -->
        <server name="WbPipelineFSP" path="&BIN_DIR;/frameScriberPublisher"
            directory="&DEFAULT_DIR;" mode="&STATE;" 
            args="&LOG;  &USE_DB; subsystem=wbpipeline delay=400" 
            system="data" startup-priority="&monitor;"/>
            
        <server name="WbDataCatcher" path="&BIN_DIR;/catchData"
            directory="&DEFAULT_DIR;" mode="&STATE;" 
            args="&LOG;  &USE_DB; mode=wb" 
            optargs="-ORBSvcConf &TOP_DIR;/conf/tao/fc.conf"
            system="data" critical="true" 
            startup-priority="&subsystem;"/> 
            
        <server name="WbPipeline" path="&BIN_DIR;/pipeline"
            directory="&DEFAULT_DIR;" mode="&STATE;" 
            args="&LOG; &USE_DB; mode=wb vbdir=/opt/visbrick monitorDelay=350" 
            system="data" critical="true"
            startup-priority="&subsystem;"/>

        <!-- Wideband Correlator Sim Servers -->
        <!-- Move up or down to add or remove Correlator Sim Servers 
        <server name="WbcBand2FSP"
            path="&BIN_DIR;/frameScriberPublisher"
            directory="&DEFAULT_DIR;"
            args="&LOG; &USE_DB; subsystem=wbcBand2"
            mode="&STATE;" />

        <server name="WbcBand2" 
            path="&BIN_DIR;/CorrelatorCarmaServer"
            directory="&DEFAULT_DIR;" mode="&STATE;"
            args="b=10 p=19002 f=correlator/wbcorrelator.conf sim=correlator/lab-carma8.ini hwtype=cobra"
            critical="true"/>
        -->
    
    </oad> <!-- End ACC -->
    
